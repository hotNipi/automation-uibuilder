    {
        "id": "38d1dfd4.1cf65",
        "type": "function",
        "z": "f56ccabd.7567e8",
        "name": "on ui connected",
        "func": "if(msg.from && msg.from == 'client'){\n    if(msg.uibuilderCtrl == 'ready for content' ){\n        let devices = global.get('devices') || []\n        devices.forEach(d => {\n            let device = global.get(d)\n            node.send({topic:\"deviceUpdate\", protocol:device.name, payload:{state:device.state,auto:device.auto,lastupdate:device.lastupdate}})\n        })\n        let controllers = global.get('controllers') || []\n        controllers.forEach(c => {\n            let controller = global.get(c)\n            if(controller && controller.sensors){\n                Object.keys(controller.sensors).forEach(sensor => {\n                    Object.keys(controller.sensors[sensor]).forEach(prop => {\n                        if(prop != \"name\" && prop != \"lastupdate\"){\n                            var p = [c,sensor,prop]\n                            node.send({topic:\"sensorUpdate\",protocol:p.join('.'),payload:controller.sensors[sensor][prop],timestamp:controller.sensors[sensor].lastupdate})\n                        }\n                    })\n                })\n            }\n        })\n    }\n}\n",
