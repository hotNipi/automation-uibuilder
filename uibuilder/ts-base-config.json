{
    "compilerOptions": {
      "lib": ["es6","es7", "dom"],
      "target": "es5",
      "noEmitOnError":true,
      "sourceMap": false,
      "preserveConstEnums": false,
      "removeComments": true,
      "strict": true,
      "noImplicitThis": false,
      "noImplicitAny": true,
      "strictNullChecks": false,
      "strictFunctionTypes":false
